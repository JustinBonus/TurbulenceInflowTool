FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      inflowProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// mean velocity
UDict
{
    referenceValue          0;
    profile                 uniform;
}

// turbulent intensity (symmTensorField)
IDict
{
    referenceValue         (0.1  0  0  0.1  0  0.1);
    profile                 uniform;
}

// turbulence length scale profile for u component
LuxDict
{
    referenceValue          0;
    profile                 uniform;
}

// turbulence length scale profile for v component
LvxDict
{
    referenceValue          0;
    profile                 uniform;
}

// turbulence length scale profile for w component
LwxDict
{
    referenceValue          0;
    profile                 uniform;
}

LuyToLuxRatio              0;
LuzToLuxRatio              0;
LvyToLvxRatio              0;
LvzToLvxRatio              0;
LwyToLwxRatio              0;
LwzToLwxRatio              0;


