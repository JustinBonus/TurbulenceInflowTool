        // mean velocity
        U_Profile
        {
                        referenceValue    (000);
                        profile           linear;
                        direction         (100);
                        origin            (000);
                        referencePoint    (000);
                        alpha             (0.10.10.1);
        }


        // turbulent intensity (symmTensorField)
        I_Profile
        {
                        profile           linear;
                        referenceValue    (0.1000.100.1);
                        direction         (100);
                        origin            (000);
                        referencePoint    (000);
                        alpha             (0.10.10.1);
        }

        // turbulence length scale profile for u component
        Lu_Profile
        {
                        profile           linear;
                        referenceValue    (000);
                        direction         (100);
                        origin            (000);
                        referencePoint    (000);
                        alpha             (0.10.10.1);
        }

        // turbulence length scale profile for v component
        Lv_Profile
        {
                        profile           linear;
                        referenceValue    (000);
                        direction         (100);
                        origin            (000);
                        referencePoint    (000);
                        alpha             (0.10.10.1);
        }

        // turbulence length scale profile for w component
        Lw_Profile
        {
                        profile           linear;
                        referenceValue    (000);
                        direction         (100);
                        origin            (000);
                        referencePoint    (000);
                        alpha             (0.10.10.1);
        }


